<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CP_FirstNameRequired</fullName>
    <active>true</active>
    <description>El campo Nombre(s) debe ser requerido.</description>
    <errorConditionFormula>AND( ISBLANK( FirstName )
    ,$Permission.CP_CatchAccountVR = TRUE
   )</errorConditionFormula>
    <errorDisplayField>FirstName</errorDisplayField>
    <errorMessage>El Nombre(s) es requerido.</errorMessage>
</ValidationRule>
